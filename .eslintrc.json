{
    "extends": ["airbnb"],
    "parserOptions": {
      "ecmaVersion": 2017
  },

  "env": {
      "es6": true
  },
    "rules": {
      "no-console": "off",
      "linebreak-style":"off",
      "import/no-dynamic-require": "off",
      "global-require": "off",
      "consistent-return": "off"
    }
}